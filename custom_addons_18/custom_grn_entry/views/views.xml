<odoo>
  <data>
      <record id="view_move_form_lc_pass" model="ir.ui.view">
        <field name="name">account.move.form.pass</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
          <field name="arch" type="xml">

              <xpath expr="//field[@name='ref']" position="after">
                  <field name="pass_entry" invisible="1"/>
              </xpath>

        </field>
     </record>

    <record id="stock_picking_form_mrp_subcontracting" model="ir.ui.view">
        <field name="name">stock.picking.inherited.form.mrp.subcontracting</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('oe_button_box')]" position="inside">
              <button name="action_get_bill_view"
                    class="oe_stat_button"
                    icon="fa-file-text-o"
                    type="object">
                    <field name="bills_count" widget="statinfo" string="Journal Entry"/>
                </button>
            </xpath>
        </field>
    </record>
      <record id="purchase_order_form_inherit_custom_grn" model="ir.ui.view">
        <field name="name">purchase.order.form.inherit</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='currency_id']" position="after">
                <field name="enable_grn"/>
            </xpath>

        </field>
      </record>



  </data>
</odoo>