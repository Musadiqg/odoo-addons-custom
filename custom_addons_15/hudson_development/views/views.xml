     <record id="cron_set_entries" model="ir.cron">
        <field name="name">Moves: Adjust entries</field>
        <field name="model_id" ref="model_account_move"/>
        <field name="state">code</field>
        <field name="code">model._run_adjust_entries_cron()</field>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
       <field name="active">False</field>
    </record>



    <record id="cron_set_fiscals" model="ir.cron">
        <field name="name">Fiscal: set fiscal positions</field>
        <field name="model_id" ref="model_account_fiscal_position"/>
