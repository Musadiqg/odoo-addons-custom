<odoo>
  <data>

     <record id="view_stock_move_line_detailed_operation_tree_batch_no" model="ir.ui.view">
        <field name="name">stock.move.line.operations.inherit.tree.batch</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='lot_id']" position="after">
                <field name="picking_type_use_existing_lots" invisible="1"/>
                <field name="supp_batch" force_save="1" attrs="{
                    'column_invisible': [('parent.picking_type_code', '!=', 'incoming')]}"/>
            </xpath>
        </field>
    </record>


       <record id="cron_set_reserve_revert" model="ir.cron">
        <field name="name">Revert Reserve Qty: set stock quants</field>
        <field name="model_id" ref="model_stock_quant"/>
        <field name="state">code</field>
        <field name="code">model.revert_reserve()</field>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
       <field name="active">False</field>
    </record>

