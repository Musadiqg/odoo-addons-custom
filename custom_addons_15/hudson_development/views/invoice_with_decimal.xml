<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

         <record id="ir_hud_dev_hudson" model="ir.actions.report">
            <field name="name">Invoice Service/Others (Round Off)</field>
            <field name="model">account.move</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">hudson_development.report_invoice_with_payments_hudson</field>
            <field name="report_file">hudson_development.report_invoice_with_payments_hudson</field>
            <field name="binding_model_id" ref="account.model_account_move"/>
            <field name="binding_type">report</field>
        </record>

        <template id="report_invoice_with_payments_hudson">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-set="lang" t-value="o.partner_id.lang"/>
                    <t t-set="print_with_payments" t-value="True"/>
                    <t t-if="o._get_name_invoice_report() == 'account.report_invoice_document'" t-call="hudson_development.report_invoice_document_decimal_hudson" t-lang="lang"/>
                </t>
            </t>
        </template>



    </data>
</odoo>