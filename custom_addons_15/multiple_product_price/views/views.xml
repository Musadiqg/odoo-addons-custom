<odoo>
  <data>

        <record id="view_crm_partner_geo_form_registered" model="ir.ui.view">
            <field name="name">res.partner.registered.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vat']" position="after">
                     <field name="is_registered"/>
                </xpath>
            </field>
        </record>

     <record id="product_product_ext_hudson" model="ir.ui.view">
            <field name="name">product_extended.product.form.view.hudson</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view" />
            <field name="arch" type="xml">
               <xpath expr="//field[@name='uom_id']" position="before">
                    <field name="x_x_price"/>
                </xpath>
            </field>
     </record>

